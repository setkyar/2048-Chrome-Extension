{
  "manifest_version": 2,
  "name": "2048",
  "version": "1.1",

  "default_locale": "en",
  "description": "You can play 2048 Game Offline through this extension",
  "permissions": [
    "bookmarks"
  ],
  "browser_action": {
      "default_title": "2048",
      "default_icon": "icon.png",
      "default_popup": "popup.html"
  },

  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}
